{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "plugins": ["prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "import/extensions": "off",
    "max-len": "off",
    "object-curly-newline": "off"
  }
}
